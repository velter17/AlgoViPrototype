<?xml version="1.0" encoding="UTF-8" ?>
<TestSettings>
	<TestProvider>
		<MaxTestLen>50</MaxTestLen>  <!-- do not display test with more than maxTestLen symbols -->
		<Colors>
			<Border>#A0A0A0</Border>
			<InputTitle>#BFA8FF</InputTitle>
			<OutputTitle>#FF9999</OutputTitle>
		</Colors>
	</TestProvider>
	<Tester>
		<TestlibChecker>
			<Path>checkers/wcmp</Path>
			<Description>compare sequences of tokens</Description>
		</TestlibChecker>
		<TestlibChecker>
			<Path>checkers/hcmp</Path>
			<Description>compare two huge signed integers</Description>
		</TestlibChecker>
		<TestlibChecker>
			<Path>checkers/icmp</Path>
			<Description>compare two signed integers</Description>
		</TestlibChecker>
		<TestlibChecker>
			<Path>checkers/lcmp</Path>
			<Description>compare file as sequence of tokens in lines</Description>
		</TestlibChecker>
		<TestlibChecker>
			<Path>checkers/ncmp</Path>
			<Description>compare ordered sequences of signed integers</Description>
		</TestlibChecker>
		<TestlibChecker>
			<Path>checkers/rcmp</Path>
			<Description>compare two doubles</Description>
		</TestlibChecker>
		<TestlibChecker>
			<Path>checkers/rcmp4</Path>
			<Description>compare two doubles, eps = 1e-4</Description>
		</TestlibChecker>
		<TestlibChecker>
			<Path>checkers/rcmp6</Path>
			<Description>compare two doubles, eps = 1e-6</Description>
		</TestlibChecker>
		<TestlibChecker>
			<Path>checkers/rcmp9</Path>
			<Description>compare two doubles, eps = 1e-9</Description>
		</TestlibChecker>
		<TestlibChecker>
			<Path>checkers/uncmp</Path>
			<Description>compare unordered sequences of integers</Description>
		</TestlibChecker>
		<TestlibChecker>
			<Path>checkers/yesno</Path>
			<Description>compare yes/no messages</Description>
		</TestlibChecker>
		<Colors>
			<Accepted>#00FF00</Accepted>
			<WrongAnswer>#FF0000</WrongAnswer>
			<RuntimeError>#CF8F18</RuntimeError>
			<CheckFailed>#CF8F18</CheckFailed>
		</Colors>
	</Tester>
</TestSettings>
