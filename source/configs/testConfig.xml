<?xml version="1.0" encoding="UTF-8" ?>
<TestSettings>
	<TestProvider>
		<MaxTestLen>50</MaxTestLen>  <!-- do not display test with more than maxTestLen symbols -->
		<Colors>
            <Color name = "border">#A0A0A0</Color>          <!-- color of test border -->
            <Color name = "input_title">#BFA8FF</Color>     <!-- color of word "Input" -->
            <Color name = "output_title">#FF9999</Color>    <!-- color of word "Output" -->
		</Colors>
	</TestProvider>
	<Tester>
		<Checker path="checkers/wcmp">compare sequences of tokens</Checker>
		<Checker path="checkers/hcmp">compare two huge signed integers</Checker>
		<Checker path="checkers/icmp">compare two signed integers</Checker>
		<Checker path="checkers/lcmp">compare file as sequence of tokens in lines</Checker>
		<Checker path="checkers/ncmp">compare ordered sequences of signed integers</Checker>
		<Checker path="checkers/rcmp">compare two doubles</Checker>
		<Checker path="checkers/rcmp4">compare two doubles, eps = 1e-4</Checker>
		<Checker path="checkers/rcmp6">compare two doubles, eps = 1e-6</Checker>
		<Checker path="checkers/rcmp9">compare two doubles, eps = 1e-9</Checker>
		<Checker path="checkers/uncmp">compare unordered sequences of integers</Checker>
		<Checker path="checkers/yesno">compare yes/no messages</Checker>
		<Colors>
			<Color name = "accepted">#00FF00</Color>
			<Color name = "wrong_answer">#FF0000</Color>
			<Color name = "runtime_error">#CF8F18</Color>
			<Color name = "check_failed">#CF8F18</Color>
		</Colors>
	</Tester>
</TestSettings>
